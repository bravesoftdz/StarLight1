  object Q_EMPL: TOraQuery
    SQLUpdate.Strings = (
      'begin'
      
        '  update client_rule set enable_view = :enable_view where id_cli' +
        'ents = :id_clients;'
      '  if sql%notfound then'
      '    insert into client_rule values(:id_clients, :enable_view);'
      '  end if;'
      'end;')
    SQLRefresh.Strings = (
      
        'select nvl(enable_view,0) as enable_view from client_rule where ' +
        'id_clients = :id_clients;')
    SQL.Strings = (
      
        'select c.id_clients, c.nick, c.fio, nvl(d.enable_view,0) as enab' +
        'le_view from ('
      'SELECT distinct b.id_employees'
      
        '  FROM admin_programs_role_groups a, admin_employees_role_groups' +
        ' b'
      '  where a.id_programs = 17 and a.c_start = 1'
      '  and a.id_role_groups = b.id_role_groups'
      '  )'
      'a, clients c, client_rule d'
      'where a.id_employees = c.id_clients'
      'and c.id_office = :v_office'
      'and c.id_clients = d.id_clients(+)'
      'order by c.fio')
    BeforeOpen = Q_EMPLBeforeOpen
    BeforeRefresh = Q_EMPLBeforeOpen
