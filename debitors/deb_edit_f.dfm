  object CDS_A_DEB: TOraQuery
    SQL.Strings = (
      'SELECT a.ID_DEBETORS, b.nick || '#39' '#39' || b.fio as fio_name '
      'FROM DEBETORS a, CLIENTS b '
      'where a.ID_CLIENTS = b.ID_CLIENTS '
      'and b.ID_CLIENTS_GROUPS = :p1'
      'and a.ID_DEBETORS <> :p2'
      'order by b.nick')
