  object DOCNEW1_DATA: TOraQuery
    SQLUpdate.Strings = (
      'begin'
      ''
      'delete from STORE_DOC_DATA_TEMP where ID_DOC_DATA=:added;'
      ''
      'IF (:QUANTITY > 0 or (:ADDED_1 = 1 and :QUANTITY >= 0)) THEN '
      
        ' INSERT INTO STORE_DOC_DATA_TEMP (ID_DOC_DATA, N_ID, QUANTITY, s' +
        'tore_type, PRICE) '
      
        ' VALUES (STORE_DOC_DATA_TEMP_SET_ID.NEXTVAL, :N_ID, :QUANTITY, :' +
        'STORE_TYPE, :PRICE);'
      'end if;  '
      ''
      'end;')
    SQLRefresh.Strings = (
      
        'select N_ID, CODE, H_CODE, F_TYPE, F_SUB_TYPE, FT_ID, FST_ID, fu' +
        'll_name, QUANTITY_NOW,'
      
        '  store_type_name, STORE_TYPE, price, price_list, QUANTITY, QUAN' +
        'TITY_PRICE, added,'
      
        '  CASE WHEN added is not NULL and added<>0 THEN 1 ELSE 0 END add' +
        'ed_1,'
      
        '  spesification, S_NAME_RU, price_difference, sum_price_differen' +
        'ce'
      
        ', case when added is null then 0 else (price * ( QUANTITY - QUAN' +
        'TITY_NOW)) end calc_price_difference'
      
        ', case when added is null then 0 else (QUANTITY - QUANTITY_NOW) ' +
        'end q_difference'
      'from '
      'store_docdatatemp_view'
      'where ID_DEPARTMENTS=:ID_DEPARTMENTS'
      'and STORE_TYPE=:STORE_TYPE'
      'and N_ID=:N_ID')
    Session = DM.OraSession
    SQL.Strings = (
      
        'select N_ID, CODE, H_CODE, F_TYPE, F_SUB_TYPE, a.FT_ID, a.FST_ID' +
        ', full_name, QUANTITY_NOW,'
      
        '  store_type_name, STORE_TYPE, price, price_list, QUANTITY, QUAN' +
        'TITY_PRICE, added,'
      
        '  CASE WHEN added is not NULL and added<>0 THEN 1 ELSE 0 END add' +
        'ed_1,'
      
        '  spesification, S_NAME_RU, price_difference, sum_price_differen' +
        'ce'
      
        ', case when added is null then 0 else (price * ( QUANTITY - QUAN' +
        'TITY_NOW)) end calc_price_difference'
      ', COMPILED_NAME_OTDEL, photo, our_code'
      
        ', case when added is null then 0 else (QUANTITY - QUANTITY_NOW) ' +
        'end q_difference'
      ', notuse'
      'from store_docdatatemp_view a'
      
        '     , (SELECT a.fst_id, a.nbutton, a.f_char FROM buttons_set a ' +
        'where id_dep = :ID_DEPARTMENTS) b'
      'where a.ID_DEPARTMENTS=:ID_DEPARTMENTS'
      '      and STORE_TYPE=:STORE_TYPE'
      '      and a.fst_id = b.fst_id(+)'
      '      and a.id_office = :v_office'
      'ORDER BY b.nbutton, a.compiled_name_otdel')
    AutoCommit = False
    RefreshOptions = [roAfterUpdate]
    Options.ReturnParams = True
    FilterOptions = [foCaseInsensitive]
    BeforeOpen = DOCNEW1_DATABeforeOpen
    AfterPost = DOCNEW1_DATAAfterPost
    BeforeRefresh = DOCNEW1_DATABeforeOpen
    Left = 680
    Top = 88
    ParamData = <
