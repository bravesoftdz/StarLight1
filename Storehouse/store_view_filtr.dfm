    end
  end
  object flower_subtypes: TOraQuery
    SQL.Strings = (
      'select * from flower_subtypes'
      'where'
      
        'FT_ID in (select FT_ID from flower_types where ID_DEPARTMENTS=:I' +
        'D_DEPARTMENTS)')
