  object flower_types: TOraQuery
    SQL.Strings = (
      'select 0 as checked, a.* from ('
      'SELECT a.ft_id, a.f_type, null as fst_id, a.id_departments'
      '  FROM s_flowers.ft_view a'
      'union all'
      
        'SELECT a.ft_id, a.f_sub_type as f_type, a.fst_id, a.id_departmen' +
        'ts'
      '  FROM s_flowers.fst_view a'
      ') a where a.id_departments = :ID_DEPARTMENTS')
    Left = 160
    Top = 56
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'ID_DEPARTMENTS'
      end>
    object flower_typesFT_ID: TFloatField
