  object qPriceHistory: TOraQuery
    Session = DM.STAR_DB
    SQL.Strings = (
      
        'SELECT a.n_id, a.price_date, a.price, a.spec_price, a.best_price' +
        ', a.discount'
      '  FROM price_list_all a'
      
        '  where n_id = :p_n_id and a.price_date between sysdate-12360 an' +
        'd sysdate and a.id_office = const_office'
      '  order by a.price_date desc')
    FetchAll = True
    Left = 136
    Top = 216
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'p_n_id'
      end>
    object qPriceHistoryN_ID: TFloatField
      FieldName = 'N_ID'
      Required = True
    end
    object qPriceHistoryPRICE_DATE: TDateTimeField
      FieldName = 'PRICE_DATE'
      Required = True
    end
    object qPriceHistoryPRICE: TFloatField
      FieldName = 'PRICE'
      Required = True
    end
    object qPriceHistorySPEC_PRICE: TIntegerField
      FieldName = 'SPEC_PRICE'
    end
    object qPriceHistoryBEST_PRICE: TIntegerField
      FieldName = 'BEST_PRICE'
    end
    object qPriceHistoryDISCOUNT: TFloatField
      FieldName = 'DISCOUNT'
    end
  end
  object cdsPriceHistory: TOraDataSource
    DataSet = qPriceHistory
    Left = 168
